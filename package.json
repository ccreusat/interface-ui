    "eslint-config-turbo": "latest",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.6.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.31.8",
    "prettier": "latest",
    "turbo": "latest",
    "typescript": "4.8.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@7.21.0",
